<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Enity.BookshelfEntity" table="bookshelf" schema="public" catalog="libraryBase">
        <id name="idBs">
            <column name="id_bs" sql-type="integer"/>
        </id>
        <property name="nameBs">
            <column name="name_bs" sql-type="varchar(40)" length="40" not-null="true"/>
        </property>
        <set name="booksByIdBs" inverse="true">
            <key>
                <column name="id_bs" not-null="true"/>
            </key>
            <one-to-many not-found="ignore" class="Enity.BookEntity"/>
        </set>
    </class>
</hibernate-mapping>